/*通用样式*/
body{
	font:12px "microsoft yahei",Arial,sans-serif;
	font-family: 'Microsoft Yahei';
	color:#666;
	margin:0;
	padding:0;
	background:#fff;
}
ul,p,h1,h2,h3,h4,h5,h6,dl,dd{
	margin:0;
	padding:0;
	list-style:none;
}
input{border:0;}
.lf{float:left;}
.rt{float:right;}
.clear{clear:both;}
img{vertical-align:bottom;}
a{
	color:#666;
	text-decoration:none;
}
a:hover{
	color:#FF0700;
	text-decoration:underline;
}
/*hpop样式*/
#hpop{
	position: relative;
    text-align: center;
    z-index: 2000;
    margin: 0px auto;
	}
	#hpop a.hpopa_1{
	display:inline-block;
	width:674.5px;
	height:85px;
    background: url("common_img/left.jpg") 100% 0px no-repeat;
	position:absolute;
	left:0;
	top:0;
	}
	#hpop a.hpopa_2{
	display:inline-block;
	width:674.5px;height:85px;
	position:absolute;
	left:50%;
	top:0;
    background: url("common_img/right.jpg") 0% 0px no-repeat;
	}
	/****页面头部样式******/
#top{background:#F2F2F2;width:1349px;height:29px;
line-height:29px;border-bottom:1px solid #ddd;;}
#top_box{float:right;margin-right:50px;}
#top_box>span{margin-right:15px;box-sizing:border-box;display:inline-block;}
#top_box>span>i{font-style:normal;padding:0 10px;color:#ccc;}
.unlogin{margin-right:10px;float:left;}
.top_nav{float:right;}
.top_nav>li{float:left;position:relative;border:1px solid transparent;box-sizing:border-box;}
.top_nav i{font-style:normal;color:#ccc;}
.top_nav li>a{padding:0 15px;text-decoration:none;}
.top_nav li .section-tigger{height:7px;width:11px;
margin-left:10px;display:inline-block;
background:url(common_img/section-tigger.png) no-repeat -30px -37px;}
.top_nav>li:hover{background:#FFF;border:1px solid #ddd;}

.top_nav li:hover i{visibility:hidden;}
.top_nav li>a{transition:color 1s linear;}
.top_nav li>a:hover{color:red;text-decoration:none;}
.top_nav li:hover .section-tigger{background:url(common_img/section-tigger.png) no-repeat -16px -28px;}
.my_okbuy{display:none;position:absolute;background:#FFF;border:1px solid #ddd;
top:29px;
z-index:10;
left:0;}
.my_okbuy li{width:90px;border:none;}

.guanzhu_okbuy{position:absolute;top:28px;left:-1px;
width:115px;text-align:center;border:1px solid #ddd;
background:#fff;z-index:20;padding-top:5px;
display:none;}
/*公告样式*/
.gongao{width:250px;position:absolute;top:28px;left:-100px;
text-align:left;border:1px solid #ddd;
background:#fff;z-index:20;padding-top:5px;
display:none;}
.gongao a{padding-left:5px;}
.top_nav>li:hover .gongao{display:block;border-top:0;}
.top_nav>li:hover .my_okbuy{display:block;}
.top_nav>li:hover .guanzhu_okbuy{display:block;border-top:0;}

/*top_main*/
#top_main{width:1200px;margin:0 auto;height:110px;}

/**STEP2 LOGO和搜索框**/
#top_main_box{background:#F5F5F5;}
#top_main{background:#F5F5F5;
	padding:10px 0;
}
/*搜索框 */
#search_box{
	/*宽度，左右外边距，左浮动*/
	width:500px;
	margin:28px 80px 0 60px;
	float:left;
	position:relative;
}
/*搜索框-搜索底部样式*/
#search_box div.search{
	/*宽度，高度，背景颜色*/
	width:500px;
	height:30px;
	background:#E4393C;
	/*边框/或内边距*/
	padding:3px;
}
#search_box div.search input{
	/*宽度，高度，padding设置为0，左内边距*/
	width:410px;
	height:30px;
	padding:0;
	padding-left:10px;
}
/*搜索框中的搜索按钮*/
#search_box div.search .btn_search{
	/*去边框，宽度，高度，背景，文字：大小，颜色，加粗*/
	border:0;
	width:45px;
	height:28px;
	background:#E4393C;
	
	background-size:50%;
	margin-left:20px;
	display:inline-block;
	font-size:14px;
	color:#fff;
	font-weight:bold;
	cursor:pointer;
}
/*search_btn*/


/*搜索框-热门搜索*/
#search_box div.hot_words{
	/*上外边距，文本颜色*/
	margin-top:5px;
	color:#999;
	/*宽度，高度，溢出隐藏*/
	width:100%;
	height:16px;
	overflow:hidden;
}
#search_box div.hot_words a{
	/*文本颜色，左外边距*/
	color:#999;
	margin-left:5px;
}
/*LOGO和搜索框-我的购物车*/
#my_gwc{float:left;width:248px;height:32px;border:2px solid #FFDA74;
position:relative;box-sizing:border-box;background:#fff;
top:29px;
line-height:32px;
text-align:center;;
padding-right:20px;}
#my_gwc>a{display:inline-block;height:28px;width:175px;
line-height:32px;padding-left:50px;
background:url(common_img/common-thumb.png) no-repeat -238px -4px;}
#my_gwc>a>span{position:relative;
top:-2px;}


/*nav样式*/
#nav{width:1349px;;height:40px;background:#000;line-height:40px;}
.w1200_ul{width:1200px;margin:0 auto;}
.w1200_ul>li{float:left;line-height:40px;margin-right:10px;}
.w1200_ul>li>a{width:100px;
height:40px;
line-height:40px;
color:#fff;
font-size:14px;
text-align:center;
display:block;
font-family:"Microsoft Yahei";
}
.w1200_ul>li>a{transition:color 1s linear;}
.w1200_ul>li>a:hover{color:red;text-decoration:none;}
/*除了第一个a不变颜色样式*/
.w1200_ul>li>.index:hover{color:#fff;}

.w1200_ul li .index{background:#D70057;}
.w1200_ul li .temai{background: url(common_img/common-thumb.png) no-repeat -187px -46px;
    
    padding-top: 6px;
    position: relative;
    top: -6px;
    width: 130px;
};
}
.w1200_ul>li.quanqiu{
	margin-right:-28px;
}
.prodConTop{position: relative;
width: 1200px;
margin: 0 auto;
display:block;}
/*****footer***/

#foot_box{
	/*上下边框，上外边距，上下内边距,宽度,背景颜色*/
	background:#FFF;
	
	border-left:0;
	border-right:0;
	margin-top:50px;
	margin-bottom:20px;
	padding:5px 0;
	/*overflow:撑起当前元素高度*/
	overflow:hidden;
	width:1200px;
	margin:0 auto;
}

#foot_box ul{
	/*右浮动，宽度*/
	float:right;
	width:100px;
	margin-left:20px;
}
#foot_box ul b{
	line-height:30px;
}
#foot_box ul li{
	line-height:22px;
}



/*STEP4 : 底部 - 备案号*/
#footer{
	text-align:center;
	margin-top:10px;
}
#footer .links a{
	margin:0 5px;
}
#footer div.copyright{
	/*上下外边距，行高*/
	margin:10px 0 15px 0;
	line-height:20px;
}
#footer div.imgs a{
	margin:0 5px;
}
.w1200{width:1200px;margin:20px auto 0;background:#f5f5f5;}
